<launch>
  <rosparam param="sim">false</rosparam>
  <rosparam param="map">true</rosparam>
  <rosparam param="use_tf">true</rosparam>

  <node pkg="aa274_final" type="pose_controller.py" name="turtlebot_pose_controller" />
  <node pkg="aa274_final" type="cmd_pose_commander.py" name="cmd_pose_commander" />
  <node pkg="aa274_final" type="detector.py" name="detector" />
  <node pkg="aa274_final" type="detector_viz.py" name="detector_viz" />
  <node pkg="aa274_final" type="objectLocationTracker.py" name="location_tracker" />

  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find aa274_final)/rviz/project_sim.rviz"/>
</launch>
